{
  "name": "@my-scope/atp-agent",
  "version": "0.0.1",
  "private": true,
  "description": "ATP Agent - A2A provider server for smart agent communication endpoints",
  "scripts": {
    "prebuild": "cd ../.. && pnpm --filter @my-scope/core build",
    "dev": "wrangler dev src/server.ts",
    "dev:node": "tsx src/server.ts",
    "deploy": "wrangler deploy",
    "build": "echo \"@my-scope/atp-agent: no-op build (Express server)\"",
    "start": "NODE_ENV=production tsx src/server.ts",
