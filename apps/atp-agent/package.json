  },
  "dependencies": {
    "@agentic-trust/8004-ext-sdk": "^1.0.34",
    "@agentic-trust/8004-sdk": "^1.0.34",
    "@agentic-trust/core": "^1.0.34",
    "@hono/node-server": "^1.19.6",
    "@my-scope/core": "workspace:*",
    "dotenv": "^16.4.5",
    "hono": "^4.10.7",
    "viem": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^9.0.0",
    "tsx": "^4.20.0",
    "typescript": "^5.6.0"
  },
  "engines": {
    "node": ">=18.17.0"
  }
}
